	width: 2px;
	border-top: 1px solid #393939;
	border-bottom: 1px solid #959595;
}

QMainWindow::separator:vertical
{
    background: #ff0000;
    max-height: 2px;
	height: 2px;
	border-left: 1px solid #393939;
